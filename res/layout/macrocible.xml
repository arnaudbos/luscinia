<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:id="@+id/scrollView1"
	>
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		>
		<LinearLayout
			android:id="@+id/linearLayout1"
			android:layout_height="wrap_content"
			android:layout_width="fill_parent"
			>
			<CheckBox
				android:id="@+id/checkBox1"
				android:text="CheckBox"
				android:layout_height="wrap_content"
				android:layout_width="fill_parent"
				android:layout_weight="1"
				>
			</CheckBox>
			<CheckBox
				android:id="@+id/checkBox2"
				android:text="CheckBox"
				android:layout_height="wrap_content"
				android:layout_width="fill_parent"
				android:layout_weight="1"
				>
			</CheckBox>
			<CheckBox
				android:id="@+id/checkBox3"
				android:text="CheckBox"
				android:layout_height="wrap_content"
				android:layout_width="fill_parent"
				android:layout_weight="1"
				>
			</CheckBox>
		</LinearLayout>
		<RelativeLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			>
			<TextView
				android:id="@+id/mTextView"
				android:text="TextView"
				android:layout_alignParentTop="true"
				android:scrollHorizontally="false"
				android:layout_width="100dip"
				android:gravity="center"
				android:layout_height="fill_parent"
				>
			</TextView>
			<EditText
				android:text="EditText"
				android:id="@+id/mEditText"
				android:layout_toRightOf="@+id/mTextView"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:minHeight="150dip"
				android:gravity="top"
				>
			</EditText>
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			>
			<TextView
				android:id="@+id/tTextView"
				android:text="TextView"
				android:layout_alignParentTop="true"
				android:scrollHorizontally="false"
				android:layout_width="100dip"
				android:gravity="center"
				android:layout_height="fill_parent"
				>
			</TextView>
			<EditText
				android:text="EditText"
				android:id="@+id/tEditText"
				android:layout_toRightOf="@+id/tTextView"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:minHeight="150dip"
				android:gravity="top"
				>
			</EditText>
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			>
			<TextView
				android:id="@+id/vTextView"
				android:text="TextView"
				android:layout_alignParentTop="true"
				android:scrollHorizontally="false"
				android:layout_width="100dip"
				android:gravity="center"
				android:layout_height="fill_parent"
				>
			</TextView>
			<EditText
				android:text="EditText"
				android:id="@+id/vEditText"
				android:layout_toRightOf="@+id/vTextView"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:minHeight="150dip"
				android:gravity="top"
				>
			</EditText>
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			>
			<TextView
				android:id="@+id/eTextView"
				android:text="TextView"
				android:layout_alignParentTop="true"
				android:scrollHorizontally="false"
				android:layout_width="100dip"
				android:gravity="center"
				android:layout_height="fill_parent"
				>
			</TextView>
			<EditText
				android:text="EditText"
				android:id="@+id/eEditText"
				android:layout_toRightOf="@+id/eTextView"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:minHeight="150dip"
				android:gravity="top"
				>
			</EditText>
		</RelativeLayout>
		<RelativeLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			>
			<TextView
				android:id="@+id/dTextView"
				android:text="TextView"
				android:layout_alignParentTop="true"
				android:scrollHorizontally="false"
				android:layout_width="100dip"
				android:gravity="center"
				android:layout_height="fill_parent"
				>
			</TextView>
			<EditText
				android:text="EditText"
				android:id="@+id/dEditText"
				android:layout_toRightOf="@+id/dTextView"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:minHeight="150dip"
				android:gravity="top"
				>
			</EditText>
		</RelativeLayout>
	</LinearLayout>
</ScrollView>